          <head xml:id="ppra-l29-Hcscscs">Conclusio Secunda</head>
          <p xml:id="ppra-l29-scdnaf">
            <assertions>
              <assertion property="isRelatedTo" target="http://scta.info/resource/l29-scdnaf"/>
              <assertion property="isRelatedTo" target="http://scta.info/resource/ppr-l29-scdnaf"/>
