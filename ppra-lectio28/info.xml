      <div xml:id="ppra-l28-Dd1e113">
        <head xml:id="ppra-l28-Hudragp">Utrum Deus reponatur in aliquo praedicamento vel in aliquo
          genere praedicamentorum</head>
        <p xml:id="ppra-l28-iirig">
          <assertions>
            <assertion property="isRelatedTo" target="http://scta.info/resource/l28-iirig"/>
            <assertion property="isRelatedTo" target="http://scta.info/resource/ppr-l28-iirig"/>
          </assertions>
        </p>
        <p xml:id="ppra-l28-cimpdp">
          <assertions>
            <assertion property="isRelatedTo" target="http://scta.info/resource/l28-cimpdp"/>
            <assertion property="isRelatedTo" target="http://scta.info/resource/ppr-l28-cimpdp"/>
